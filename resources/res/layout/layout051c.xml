<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:id="@+id/search_fragment" android:background="@color/color065f" android:layout_width="match_parent" android:layout_height="match_parent">
    <com.obwhatsapp.BidiToolbar android:layout_gravity="center_vertical" android:id="@+id/toolbar" android:layout_height="55dp" style="@style/style0380">
        <com.obwhatsapp.search.views.TokenizedSearchInput android:id="@+id/search_input_layout" android:layout_width="match_parent" android:layout_height="match_parent"/>
