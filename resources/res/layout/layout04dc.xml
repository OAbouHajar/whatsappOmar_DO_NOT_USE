                    <com.obwhatsapp.WaTextView android:textSize="14sp" android:textColor="@color/color01e8" android:id="@+id/name_counter_tv" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="10dp" android:layout_alignBaseline="@+id/registration_name" android:layout_alignParentRight="true"/>
                </RelativeLayout>
                <com.obwhatsapp.WaImageButton android:id="@+id/emoji_btn" android:layout_width="48dp" android:layout_height="48dp" android:src="@drawable/ib_emoji" android:layout_alignParentRight="true" android:layout_centerVertical="true" android:contentDescription="@string/str1c81" app:attr0532="@color/color04ca" style="@style/style0000"/>
