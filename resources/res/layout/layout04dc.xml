                    <com.obwhatsapp.components.button.ThumbnailPickerButton android:id="@+id/change_photo_btn" android:layout_width="@dimen/dimen0694" android:layout_height="@dimen/dimen0694" android:src="@drawable/avatar_contact" android:scaleType="fitCenter" android:contentDescription="@string/str1d6a" app:attr0500="@dimen/dimen0692"/>
                    <ProgressBar android:layout_gravity="center" android:id="@+id/change_photo_progress" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" style="?android:attr/progressBarStyleInverse"/>
