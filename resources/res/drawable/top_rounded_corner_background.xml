            <stroke android:width="1dp" android:color="@color/color088c"/>
            <solid android:color="@color/color0792"/>
            <padding android:left="@dimen/dimen0764" android:top="@dimen/dimen0764" android:right="@dimen/dimen0764" android:bottom="@dimen/dimen0764"/>
        </shape>
    </item>
