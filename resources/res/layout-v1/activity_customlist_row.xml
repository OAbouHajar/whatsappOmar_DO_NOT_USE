<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:padding="@dimen/list_row_padding" android:layout_width="match_parent" android:layout_height="wrap_content">
    <LinearLayout android:orientation="vertical" android:padding="2dp" android:layout_width="wrap_content" android:layout_height="wrap_content">
        <ImageView android:id="@+id/image" android:tag="i" android:layout_width="40dp" android:layout_height="40dp" android:src="@drawable/attach_contact" android:contentDescription="@string/str15b0"/>
    </LinearLayout>
    <LinearLayout android:orientation="vertical" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:layout_weight="2">
        <com.obwhatsapp.yo.tf android:textSize="15dp" android:textStyle="bold" android:id="@+id/name" android:tag="n" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/profile_name"/>
        <com.obwhatsapp.yo.tf android:id="@+id/description" android:tag="d" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/str0435"/>
    </LinearLayout>
    <CheckBox android:layout_gravity="center_vertical" android:id="@+id/cbx" android:tag="c" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
    <ImageView android:layout_gravity="center_vertical" android:id="@+id/div2" android:tag="r" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@android:drawable/ic_delete" android:contentDescription="Delete"/>
</LinearLayout>
