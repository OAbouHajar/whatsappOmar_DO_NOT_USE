<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:fab="http://schemas.android.com/apk/com.getbase" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
    <View android:background="@drawable/list_separator_bot" android:layout_width="match_parent" android:layout_height="@dimen/info_screen_card_spacing"/>
    <LinearLayout android:orientation="vertical" android:id="@+id/sprivacy" android:background="@color/primary_surface" android:layout_width="match_parent" android:layout_height="wrap_content">
        <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/privacyOnly" android:background="@drawable/selector_orange_gradient" android:paddingLeft="@dimen/info_screen_padding" android:paddingRight="12dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:minHeight="@dimen/info_screen_row">
            <LinearLayout android:gravity="left|center_vertical|center_horizontal|center" android:orientation="vertical" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1">
                <com.obwhatsapp.yo.tf android:layout_gravity="left" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/specificPrivacy" style="@style/ListItemTitle"/>
                <com.obwhatsapp.yo.tf android:id="@+id/sprivacy_enabled" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/specificPrivacy_sum" style="@style/ListItemSubtitleSmall"/>
            </LinearLayout>
            <androidx.appcompat.widget.SwitchCompat android:id="@+id/sprivacy_switch" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
        </LinearLayout>
        <View style="@style/FlatCardSeparator"/>
        <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/MaskMedia" android:background="@drawable/selector_orange_gradient" android:paddingLeft="@dimen/info_screen_padding" android:paddingRight="12dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:minHeight="@dimen/info_screen_row">
            <LinearLayout android:gravity="left|center_vertical|center_horizontal|center" android:orientation="vertical" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1">
                <com.obwhatsapp.yo.tf android:layout_gravity="left" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/Mask_media" style="@style/ListItemTitle"/>
                <com.obwhatsapp.yo.tf android:id="@+id/smedia_enabled" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/specificMedia_sum" style="@style/ListItemSubtitleSmall"/>
            </LinearLayout>
            <androidx.appcompat.widget.SwitchCompat android:id="@+id/smask_switch" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
        </LinearLayout>
        <View style="@style/FlatCardSeparator"/>
        <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/masked" android:background="@drawable/selector_orange_gradient" android:paddingLeft="@dimen/info_screen_padding" android:paddingRight="12dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:minHeight="@dimen/info_screen_row">
            <LinearLayout android:gravity="left|center_vertical|center_horizontal|center" android:orientation="vertical" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1">
                <com.obwhatsapp.yo.tf android:layout_gravity="left" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/yoHideProfileName" style="@style/ListItemTitle"/>
            </LinearLayout>
            <androidx.appcompat.widget.SwitchCompat android:id="@+id/main_text" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
        </LinearLayout>
        <View style="@style/FlatCardSeparator"/>
        <LinearLayout android:gravity="left|center_vertical|center_horizontal|center" android:orientation="horizontal" android:id="@+id/modPassword" android:background="@drawable/selector_orange_gradient" android:paddingLeft="@dimen/info_screen_padding" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_weight="1">
            <LinearLayout android:gravity="left|center_vertical|center_horizontal|center" android:layout_gravity="left|center_vertical|center_horizontal|center" android:orientation="vertical" android:paddingTop="10dp" android:paddingBottom="12dp" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1">
                <com.obwhatsapp.yo.tf android:layout_gravity="left" android:id="@+id/title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/str10e8" style="@style/ListItemTitle"/>
                <com.obwhatsapp.yo.tf android:textSize="14sp" android:textColor="@color/list_item_info" android:gravity="left" android:layout_gravity="left" android:id="@+id/modPasswordText" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
            </LinearLayout>
            <ImageView android:layout_gravity="center_vertical" android:id="@+id/modPasswordLogo" android:paddingLeft="20dp" android:paddingRight="12dp" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
        </LinearLayout>
        <LinearLayout android:orientation="vertical" android:id="@+id/div2" android:layout_width="wrap_content" android:layout_height="wrap_content">
            <View style="@style/FlatCardSeparator"/>
            <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:background="@drawable/selector_orange_gradient" android:paddingLeft="@dimen/info_screen_padding" android:paddingRight="12dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:minHeight="@dimen/info_screen_row">
                <LinearLayout android:gravity="left|center_vertical|center_horizontal|center" android:orientation="vertical" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1">
                    <com.obwhatsapp.yo.tf android:layout_gravity="left" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/no_calls" style="@style/ListItemTitle"/>
                </LinearLayout>
                <androidx.appcompat.widget.SwitchCompat android:id="@+id/call_spam_block" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
            </LinearLayout>
            <View style="@style/FlatCardSeparator"/>
            <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:background="@drawable/selector_orange_gradient" android:paddingLeft="@dimen/info_screen_padding" android:paddingRight="12dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:minHeight="@dimen/info_screen_row">
                <LinearLayout android:gravity="left|center_vertical|center_horizontal|center" android:orientation="vertical" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1">
                    <com.obwhatsapp.yo.tf android:layout_gravity="left" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/yo_online_toast" style="@style/ListItemTitle"/>
                </LinearLayout>
                <androidx.appcompat.widget.SwitchCompat android:id="@+id/modC" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <View android:background="@drawable/list_separator_top" android:layout_width="match_parent" android:layout_height="@dimen/info_screen_card_spacing"/>
</LinearLayout>
