<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <LinearLayout android:gravity="left|center_vertical|center_horizontal|center" android:orientation="vertical" android:id="@+id/main_layout" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="40dp" android:minHeight="30dp">
        <include layout="@layout/layout01b6"/>
        <include layout="@layout/layout01c1"/>
        <FrameLayout android:id="@+id/suspicious_link_indicator_holder" android:paddingLeft="4dp" android:paddingTop="4dp" android:paddingRight="4dp" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content"/>
        <include layout="@layout/layout01e6"/>
        <LinearLayout android:orientation="horizontal" android:id="@+id/conversation_text_row" android:paddingLeft="2dp" android:paddingBottom="2dp" android:layout_width="match_parent" android:layout_height="wrap_content">
            <FrameLayout android:layout_gravity="left" android:id="@+id/picture_frame" android:tag="pi" android:paddingLeft="2dp" android:layout_width="40dp" android:layout_height="40dp">
                <ImageView android:id="@+id/picture" android:padding="7dp" android:layout_width="match_parent" android:layout_height="match_parent" android:src="@drawable/attach_contact" android:scaleType="fitCenter"/>
