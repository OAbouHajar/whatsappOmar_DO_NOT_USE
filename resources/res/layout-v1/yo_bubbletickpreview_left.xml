<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:fab="http://schemas.android.com/apk/com.getbase" xmlns:aapt="http://schemas.android.com/aapt">
    <LinearLayout android:gravity="left|center_vertical|center_horizontal|center" android:orientation="vertical" android:id="@+id/main_layout" android:background="@drawable/balloon_incoming_normal" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="40dp" android:minHeight="30dp" android:layoutDirection="ltr">
        <LinearLayout android:layout_gravity="top|left|center_vertical|center_horizontal|center" android:id="@+id/name_in_group" android:paddingLeft="8dp" android:paddingTop="3dp" android:paddingRight="8dp" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content">
            <com.obwhatsapp.yo.tf android:textSize="@dimen/conversation_participant_name_tv" android:textColor="#ff000000" android:id="@+id/name_in_group_tv" android:paddingRight="4sp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="" android:lines="1" android:singleLine="true"/>
            <com.obwhatsapp.yo.tf android:textSize="@dimen/conversation_participant_name_tv" android:textColor="#ff999999" android:id="@+id/secondary_name_in_group_tv" android:paddingLeft="4sp" android:paddingRight="4sp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:lines="1" android:singleLine="true"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/quoted_message_frame" android:layout_width="match_parent" android:layout_height="wrap_content" android:foreground="@drawable/balloon_incoming_frame">
            <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:background="@drawable/link_preview_background" android:layout_width="match_parent" android:layout_height="wrap_content" android:minWidth="120dp" android:minHeight="48dp">
                <View android:id="@+id/quoted_color" android:background="#ffa35ea5" android:layout_width="3dp" android:layout_height="match_parent"/>
                <LinearLayout android:orientation="vertical" android:paddingLeft="8dp" android:paddingTop="4dp" android:paddingRight="8dp" android:paddingBottom="4dp" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1">
                    <LinearLayout android:layout_gravity="left" android:orientation="horizontal" android:id="@+id/quoted_title_frame" android:layout_width="wrap_content" android:layout_height="wrap_content">
                        <com.obwhatsapp.yo.tf android:textColor="#ffa35ea5" android:ellipsize="end" android:id="@+id/quoted_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/assem_div" android:maxLines="1" android:singleLine="true"/>
                        <com.obwhatsapp.yo.tf android:textColor="#ffa35ea5" android:ellipsize="end" android:id="@+id/quoted_bullet_divider" android:paddingLeft="4sp" android:paddingRight="4sp" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxLines="1" android:singleLine="true"/>
                        <com.obwhatsapp.yo.tf android:textColor="#ffa35ea5" android:ellipsize="end" android:id="@+id/quoted_subtitle" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/str056f" android:maxLines="1" android:singleLine="true"/>
                    </LinearLayout>
                    <FrameLayout android:layout_width="wrap_content" android:layout_height="wrap_content">
                        <com.obwhatsapp.yo.tf android:textColor="?android:attr/textColorSecondary" android:ellipsize="end" android:layout_gravity="left" android:id="@+id/quoted_text" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/assem_div_sum"/>
                    </FrameLayout>
                    <com.obwhatsapp.yo.tf android:textColor="?android:attr/textColorSecondary" android:ellipsize="end" android:layout_gravity="left" android:id="@+id/quoted_sub_text" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxLines="2"/>
                </LinearLayout>
                <ImageView android:id="@+id/quoted_thumb" android:layout_width="52dp" android:layout_height="match_parent" android:scaleType="centerCrop"/>
                <ImageView android:id="@+id/contact_photo" android:visibility="gone" android:layout_width="@dimen/conversation_profile_photo_size" android:layout_height="@dimen/conversation_profile_photo_size" android:scaleType="fitCenter"/>
            </LinearLayout>
        </FrameLayout>
        <FrameLayout android:id="@+id/suspicious_link_indicator_holder" android:paddingLeft="4dp" android:paddingTop="4dp" android:paddingRight="4dp" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content"/>
        <FrameLayout android:id="@+id/web_page_preview_holder" android:paddingLeft="4dp" android:paddingTop="4dp" android:paddingRight="4dp" android:paddingBottom="4dp" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content"/>
        <LinearLayout android:orientation="horizontal" android:paddingLeft="2dp" android:paddingBottom="2dp" android:layout_width="match_parent" android:layout_height="wrap_content">
            <FrameLayout android:layout_gravity="left" android:id="@+id/picture_frame" android:tag="pi" android:paddingLeft="2dp" android:visibility="gone" android:layout_width="40dp" android:layout_height="40dp">
                <ImageView android:id="@+id/picture" android:padding="7dp" android:layout_width="match_parent" android:layout_height="match_parent" android:src="@drawable/attach_contact" android:scaleType="fitCenter"/>
            </FrameLayout>
            <LinearLayout android:layout_gravity="center_vertical" android:orientation="vertical" android:paddingTop="2dp" android:paddingRight="4dp" android:paddingBottom="1dp" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1">
                <com.obwhatsapp.yo.tf android:textSize="@dimen/conversation_text_row_tv" android:textColor="#ff000000" android:layout_gravity="left" android:id="@+id/message_text" android:paddingLeft="8dp" android:paddingTop="2dp" android:paddingRight="8dp" android:paddingBottom="3dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/assem_themes"/>
                <LinearLayout android:gravity="center_vertical" android:layout_gravity="bottom|right|center_vertical|center_horizontal|center" android:orientation="horizontal" android:id="@+id/date_wrapper" android:paddingLeft="6dp" android:paddingRight="6dp" android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <com.obwhatsapp.yo.tf android:id="@+id/date" android:paddingBottom="1dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="07:11 PM" style="@style/WA.TextAppearance.ConversationRowDate"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</merge>
